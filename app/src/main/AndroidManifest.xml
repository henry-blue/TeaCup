<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.teacup">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".FlashActivity"
            android:theme="@style/FlashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".BookDetailActivity" />
        <activity android:name=".WeatherActivity" />
        <activity android:name=".PhotoActivity" />
        <activity
            android:name=".GuideActivity"
            android:theme="@style/FlashTheme" />
        <activity
            android:name=".ShowPhotoActivity"
            android:theme="@style/FlashTheme" />
        <activity android:name=".ShowPhotoListActivity"
            android:theme="@style/FlashTheme" />
        <activity android:name=".MusicDetailActivity" />
        <activity android:name=".MusicPlayActivity" />
        <activity android:name=".NewsDetailActivity" />
        <activity android:name=".ReadDetailActivity" />
        <activity android:name=".ReadTopicActivity" />
        <activity android:name=".ReadCollectionActivity" />
        <activity android:name=".ListReadCollectionActivity" />
        <activity android:name=".ListReadTopicActivity" />
        <activity android:name=".FindBookActivity" />
        <activity android:name=".TingDetailActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".SettingActivity" />
        <service android:name="com.app.service.LocationService" /><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>